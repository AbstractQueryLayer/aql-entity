{
  "name": "ifcastle/aql-entity",
  "description": "Entity definition and management for Abstract Query Language",
  "keywords": [
    "aql",
    "entity",
    "orm",
    "database"
  ],
  "type": "ifcastle-aql-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Edmond Dantes",
      "email": "edmondifthen@proton.me"
    }
  ],
  "require": {
    "php": ">=8.4",
    "ifcastle/aql-aspects": "^0.1.0",
    "ifcastle/aql-dsl": "^0.1.0",
    "ifcastle/aop": "^0.1.0",
    "ifcastle/di": "^0.6.0",
    "ifcastle/design-patterns": "^0.2.20",
    "ifcastle/exceptions": "^5.0",
    "ifcastle/type-definitions": "^0.5.7"
  },
  "autoload": {
    "psr-4": {
      "IfCastle\\AQL\\Entity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IfCastle\\AQL\\Entity\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.2",
    "rector/rector": "^1.2",
    "ifcastle/codestyle": "^0.2.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit ./tests",
    "cs-fix": "php-cs-fixer fix",
    "cs-check": "php-cs-fixer fix --dry-run --diff",
    "rector": "rector process",
    "rector-dry": "rector process --dry-run"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.8.x-dev"
    }
  }
}
